<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.Login">
    <children>
        <Label fx:id="welcomeMessage" alignment="CENTER" layoutX="160.0" prefHeight="100.0" prefWidth="380.0">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Label fx:id="usernameLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="100.0" layoutY="120.0" prefHeight="25.0" prefWidth="150.0">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="passwordLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="136.0" layoutY="160.0" prefHeight="25.0" prefWidth="114.0">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Button fx:id="loginButton" contentDisplay="CENTER" layoutX="290.0" layoutY="246.0" mnemonicParsing="false" onAction="#attemptLogin" prefHeight="40.0" prefWidth="150.0">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <TextField fx:id="passwordField" layoutX="267.0" layoutY="160.0" prefHeight="25.0" prefWidth="200.0" />
        <TextField fx:id="usernameField" layoutX="267.0" layoutY="120.0" prefHeight="25.0" prefWidth="200.0" />
    </children>
</AnchorPane>
