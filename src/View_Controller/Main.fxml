<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.Main">
   <children>
      <Button fx:id="custTableButton" layoutX="170.0" layoutY="200.0" mnemonicParsing="false" onAction="#customerTableEvent" prefHeight="40.0" prefWidth="370.0" text="Customer List">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label layoutX="160.0" prefHeight="100.0" prefWidth="380.0" text="Event Calendar Options" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="apptsTableButton" layoutX="170.0" layoutY="280.0" mnemonicParsing="false" onAction="#appointmentsTableEvent" prefHeight="40.0" prefWidth="370.0" text="Appointment List">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="reportsButton" layoutX="170.0" layoutY="360.0" mnemonicParsing="false" onAction="#reportsEvent" prefHeight="40.0" prefWidth="370.0" text="Reports">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="555.0" layoutY="550.0" mnemonicParsing="false" onAction="#exitEvent" prefHeight="40.0" prefWidth="131.0" text="Exit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
