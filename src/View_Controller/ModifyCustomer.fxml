<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ModifyCustomer">
    <children>
        <Label alignment="CENTER" layoutX="160.0" prefHeight="100.0" prefWidth="380.0" text="Modify Customer">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Button contentDisplay="CENTER" layoutX="550.0" layoutY="550.0" mnemonicParsing="false" onAction="#modifyCustBackEvent" prefHeight="40.0" prefWidth="120.0" text="Back">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Label layoutX="180.0" layoutY="100.0" prefHeight="25.0" prefWidth="90.0" text="Name">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="180.0" layoutY="150.0" prefHeight="25.0" prefWidth="90.0" text="Address">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="180.0" layoutY="200.0" prefHeight="25.0" prefWidth="90.0" text="City">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="180.0" layoutY="250.0" prefHeight="25.0" prefWidth="90.0" text="Country">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="180.0" layoutY="300.0" prefHeight="25.0" prefWidth="90.0" text="Phone">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Button contentDisplay="CENTER" layoutX="400.0" layoutY="550.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="120.0" text="Save">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <ComboBox fx:id="modifyCustCity" layoutX="300.0" layoutY="200.0" prefHeight="25.0" prefWidth="200.0" />
        <TextField fx:id="modifyCustPhone" layoutX="300.0" layoutY="300.0" prefHeight="25.0" prefWidth="200.0" />
        <TextField fx:id="modifyCustName" layoutX="300.0" layoutY="100.0" prefHeight="25.0" prefWidth="200.0" />
      <TextField fx:id="modifyCustAddress" layoutX="300.0" layoutY="150.0" prefHeight="25.0" prefWidth="200.0" />
      <Label fx:id="modifyCustCountry" layoutX="300.0" layoutY="253.0" prefHeight="17.0" prefWidth="200.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
